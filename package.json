{
  "name": "gocanada-website",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run redirects:enable && react-router build --sourcemap && sentry-upload-sourcemaps --org tonymamo --project gocanada",
    "dev": "NODE_OPTIONS='--import ./instrumentation.server.mjs' npm run redirects:disable && netlify dev",
    "start": "NODE_OPTIONS='--import ./instrumentation.server.mjs' react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc -b",
    "redirects:enable": "shx cp .redirects public/_redirects",
    "redirects:disable": "shx rm -f public/_redirects"
  },
