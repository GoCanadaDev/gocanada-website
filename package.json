{
  "name": "gocanada-website",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run redirects:enable && remix build",
    "dev": "npm run redirects:disable && remix dev",
    "start": "netlify serve",
    "typecheck": "tsc -b",
    "redirects:enable": "shx cp .redirects public/_redirects",
    "redirects:disable": "shx rm -f public/_redirects"
  },
  "dependencies": {
    "@netlify/functions": "^2.0.0",
    "@netlify/remix-adapter": "^2.0.0",
    "@remix-run/react": "^2.3.1",
    "@resvg/resvg-js": "^2.6.0",
    "@sanity/asset-utils": "^1.3.0",
    "@sanity/client": "^6.8.6",
    "@sanity/demo": "^1.0.2",
    "@sanity/icons": "^2.7.0",
    "@sanity/image-url": "^1.0.2",
    "@sanity/overlays": "2.0.1",
    "@sanity/presentation": "1.0.5",
    "@sanity/preview-url-secret": "1.1.0",
    "@sanity/react-loader": "1.0.2",
    "@sanity/ui": "^1.9.3",
    "@sanity/vision": "^3.20.0",
    "@types/jsdom": "^21.1.5",
    "cross-env": "^7.0.3",
    "@faker-js/faker": "^8.3.1",
    "groq": "^3.21.0",
    "lucide-react": "^0.292.0",
    "postcss": "^8.4.32",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanity": "^3.20.0",
    "sanity-plugin-iframe-pane": "^2.6.1",
    "satori": "^0.10.11",
    "shx": "^0.3.4",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@remix-run/serve": "^2.3.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.27.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
